{
  "compilerOptions": {
    "declaration": true,
    "esModuleInterop": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "outDir": "./lib",
    "inlineSourceMap": true,
    "strict": true,
    "lib": ["ES2023"],
    "target": "ES2023",
    "types": ["@adonisjs/core"],
    "strictFunctionTypes": false,
    "skipLibCheck": true
  },
  "include": ["adonis-typings", "providers", "src", "vitest.config.mts"]
}
